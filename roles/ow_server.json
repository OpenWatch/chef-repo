{
  "name": "ow_server",
  "description": "OpenWatch base webserver. Creates ow user accounts and prepares SSL cert / key",
  "json_class": "Chef::Role",
  "default_attributes": {
  },
  "override_attributes": {
    "ow_users": {
      "admin_gid": 999,
      "active_gids": [0, 500, 999]
    }
  },
  "chef_type": "role",
  "run_list": [
    "role[ubuntu]",
    "role[users]",
    "role[nginx]",
    "recipe[ow_webserver]"
  ]
}