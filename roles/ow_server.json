{
  "name": "ow_server",
  "description": "OpenWatch base webserver",
  "json_class": "Chef::Role",
  "default_attributes": {
  },
  "override_attributes": {
    "set_fqdn" : "openwatch.net",
    "ow_users": {
      "admin_gid": 999,
      "active_gids": [0, 500, 999]
    }
  },
  "chef_type": "role",
  "run_list": [
    "recipe[hostname]",
    "role[ubuntu]",
    "role[users]",
    "role[nginx]"
  ]
}